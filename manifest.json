{
  "manifest_version": 3,
  "name": "New Tab",
  "version": "1.1",
  "description": "Customize Chrome New Tab",
  "permissions": ["storage", "favicon"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
